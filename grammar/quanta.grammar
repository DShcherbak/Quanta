@top Program { Statement* }

Statement { Keyword | Identifier | Punctuation | Number  }

@tokens {
  Keyword { "circle" | "rectangle" }
  Identifier { $[a-zA-Z_] $[a-zA-Z0-9_]* }
  Number { $[0-9]+ ("." $[0-9]+)? }
  Punctuation { $[(){};,] }
  WHITESPACE { " " | "\t" | "\n" | "\r" }
  @precedence {Keyword, Identifier}
}

@skip { WHITESPACE }